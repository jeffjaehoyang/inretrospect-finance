{
  "name": "my-what-if-stocks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "apexcharts": "^3.31.0",
    "autoprefixer": "^10.4.0",
    "firebase": "^9.6.0",
    "framer-motion": "^4.1.18-rc.26",
    "postcss-cli": "^9.0.2",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "react-select": "^5.2.1",
    "styled-components": "^5.3.3",
    "tailwindcss": "^2.2.19",
    "twin.macro": "^2.8.1",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "build:tailwind-dev": "postcss src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "build:tailwind-prod": "cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind-dev --parallel watch:tailwind start:react",
    "start:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.17",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.4.0",
    "npm-run-all": "^4.1.5"
  },
  "babelMacros": {
    "twin": {
      "config": "./src/tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "proxy": "http://localhost:5000",
  "engines": {
    "node": "16.3"
  }
}
